<Window x:Class="StudyGuideApp.ModuleCalendarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudyGuideApp"
        mc:Ignorable="d"
        Title="Module Calendar" Height="450" Width="883" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>

        <!-- Setting Background-->
        <Grid.Background>
            <ImageBrush x:Name="backgroundImg" ImageSource="C:\Users\lab_services_student\Documents\GitHub\Semester-StudyGuide-Generator_WPF\StudyGuideApp\Images\wp5.jpg" ></ImageBrush>
        </Grid.Background>

        <!--calendar design elements-->
        <Button x:Name="button" Content="Next" FontSize="12" Background="Beige" Foreground="Brown" HorizontalAlignment="Left" Margin="836,404,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button2" Content="Previous" HorizontalAlignment="Left" Background="Beige" Foreground="Brown" Margin="776,404,0,0" VerticalAlignment="Top"/>
        <Button x:Name="returnButton" Content="RETURN" FontSize="20" FontWeight="Light" Foreground="Brown" Background="Wheat" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="returnButton_Click" Height="34" Width="107"/>
        <Label Content="Sunday" HorizontalAlignment="Left" FontSize="14" Foreground="White" Margin="137,15,0,0" VerticalAlignment="Top"/>
        <Label Content="Monday" HorizontalAlignment="Left" FontSize="14" Foreground="White" Margin="240,15,0,0" VerticalAlignment="Top"/>
        <Label Content="Tuesday" HorizontalAlignment="Left" FontSize="14" Foreground="White" Margin="345,15,0,0" VerticalAlignment="Top"/>
        <Label Content="Wednesday" HorizontalAlignment="Left" FontSize="14" Foreground="White" Margin="439,15,0,0" VerticalAlignment="Top"/>
        <Label Content="Thursday" HorizontalAlignment="Left" FontSize="14" Foreground="White" Margin="552,15,0,0" VerticalAlignment="Top"/>
        <Label Content="Friday" HorizontalAlignment="Left" FontSize="14" Foreground="White" Margin="666,15,0,0" VerticalAlignment="Top"/>
        <Label Content="Saturday" HorizontalAlignment="Left" FontSize="14" Foreground="White" Margin="761,15,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
